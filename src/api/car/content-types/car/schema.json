{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "model_name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "fuel_capacity": {
      "type": "decimal",
      "required": true
    },
    "seat_capacity": {
      "type": "integer",
      "required": true,
      "default": 1,
      "min": 1
    },
    "current_rent_cost_per_day": {
      "type": "decimal",
      "required": true
    },
    "new_rent_cost_per_day": {
      "type": "decimal"
    },
    "steering_mode": {
      "type": "enumeration",
      "enum": [
        "Manual",
        "Auto",
        "Semi-Auto"
      ],
      "required": true,
      "default": "Manual"
    },
    "model_type": {
      "type": "enumeration",
      "enum": [
        "UNCLASSIFIED",
        "Sport",
        "SUV",
        "MPV",
        "Sedan",
        "Coupe",
        "Hatchback"
      ],
      "default": "UNCLASSIFIED",
      "required": true
    }
  }
}
